void TSP_ERR(){
  digitalWrite(ERR, HIGH);
  digitalWrite(BPER, HIGH);
  delay(500);
  digitalWrite(ERR, LOW);
  digitalWrite(BPER, LOW);
  delay(500);
  digitalWrite(ERR, HIGH);
  digitalWrite(BPER, HIGH);
  delay(500);
  digitalWrite(ERR, LOW);
  digitalWrite(BPER, LOW);
  delay(500);
  digitalWrite(ERR, HIGH);
  digitalWrite(BPER, HIGH);
  delay(500);
  digitalWrite(ERR, LOW);
  digitalWrite(BPER, LOW);
  delay(500);
  digitalWrite(ERR, HIGH);
}
